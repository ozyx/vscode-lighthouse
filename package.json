{
	"name": "vscode-lighthouse",
	"displayName": "vscode-lighthouse",
	"description": "Lighthouse extension for VSCode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:lighthouse.call",
		"onCommand:lighthouse.on",
		"onCommand:lighthouse.id.on",
		"onCommand:lighthouse.off",
		"onCommand:lighthouse.id.off",
		"onCommand:lighthouse.state",
		"onCommand:lighthouse.id.state",
		"onCommand:lighthouse.bri",
		"onCommand:lighthouse.id.bri",
		"onCommand:lighthouse.color",
		"onCommand:lighthouse.id.color",
		"onCommand:lighthouse.loop",
		"onCommand:lighthouse.id.loop",
		"onCommand:lighthouse.info"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "lighthouse.call",
				"title": "Lighthouse: Call"
			},
			{
				"command": "lighthouse.on",
				"title": "Lighthouse: Turn on all lights"
			},
			{
				"command": "lighthouse.id.on",
				"title": "Lighthouse: Turn on lights by IDs"
			},
			{
				"command": "lighthouse.off",
				"title": "Lighthouse: Turn off all lights"
			},
			{
				"command": "lighthouse.id.off",
				"title": "Lighthouse: Turn off lights by IDs"
			},
			{
				"command": "lighthouse.state",
				"title": "Lighthouse: Send STATE string"
			},
			{
				"command": "lighthouse.id.state",
				"title": "Lighthouse: Send STATE string to IDs."
			},
			{
				"command": "lighthouse.bri",
				"title": "Lighthouse: Send BRI val as brightness"
			},
			{
				"command": "lighthouse.id.bri",
				"title": "Lighthouse: Send BRI val as brightness to IDS."
			},
			{
				"command": "lighthouse.color",
				"title": "Lighthouse: Send hexcode of COLOR"
			},
			{
				"command": "lighthouse.id.color",
				"title": "Lighthouse: Send hexcode of COLOR to IDs"
			},
			{
				"command": "lighthouse.loop",
				"title": "Lighthouse: Put all lights in a colorloop"
			},
			{
				"command": "lighthouse.id.loop",
				"title": "Lighthouse: Put lights in a colorloop by IDs"
			},
			{
				"command": "lighthouse.info",
				"title": "Lighthouse: Info"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
